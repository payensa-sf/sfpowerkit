  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "^1.4.1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^12.7.2",
    "chai": "^4",
