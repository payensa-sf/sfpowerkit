    "@salesforce/command": "^2.1.0",
    "@salesforce/core": "^2.1.1",
    "archiver": "3.1.1",
    "child-process": "^1.0.2",
    "child-process-promise": "^2.2.1",
    "cli-ux": "^5.2.1",
    "copy-dir": "^1.1.0",
    "decompress": "^4.2.0",
    "fs-extra": "^8.1.0",
    "ignore": "^5.1.2",
    "request": "^2.88.0",
